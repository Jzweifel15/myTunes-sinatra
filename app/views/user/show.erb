<h1><%= @playlist.name %></h1>
<h3>Songs</h3>
<ol>
    <% @songs.each do |song| %>
        <li><%= song.name %></li>
    <% end %>
</ol>
<button>
    <a href="/index/<%= @playlist.id %>/edit">Edit Playlist</a>
</button>
<form action="/index/<%= @playlist.id %>" method="POST">
    <input type="hidden" id="hidden" name="_method" value="DELETE" />
    <input type="submit" id="delete-btn" value="Delete Playlist" />
</form>